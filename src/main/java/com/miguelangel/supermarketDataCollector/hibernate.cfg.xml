<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
		<property name="hibernate.connection.password">postgres</property>
		<property name="hibernate.connection.url">jdbc:postgresql://localhost:5432/supermarkets</property>
		<property name="hibernate.connection.username">postgres</property>
		<property name="hibernate.default_schema">public</property>
		<property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.Category" />
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.Product" />
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.PriceHistory" />
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.PriceHistoryId" />
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.ShoppingList" />
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.ShoppingListProduct" />
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.ShoppingListProductId" />
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.Supermarket" />
		<mapping
			class="com.miguelangel.supermarketDataCollector.entity.UserEntity" />
		<mapping
				class="com.miguelangel.supermarketDataCollector.entity.Role" />
	</session-factory>
</hibernate-configuration>
